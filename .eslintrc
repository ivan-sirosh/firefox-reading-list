{
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": ["error"],
    "semi": ["error", "always"]
  }
}